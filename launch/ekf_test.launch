<launch>
  <include file="$(find robot_localization)/launch/amcl_fuse.launch" />

  <node pkg="robot_localization" type="ekf_localization_node" name="ekf_se" clear_params="true">
    <rosparam command="load" file="$(find robot_localization)/params/ekf_test.yaml" />
    <!--  Placeholder for output topic remapping
    <remap from="odometry/filtered" to=""/>
    <remap from="accel/filtered" to=""/>
    -->
  </node>
  <node pkg="tf" type="static_transform_publisher" name="center_link_broadcaster" args="-0.032 0 0 0 0 0 1 base_link center_link 10" />
</launch>
